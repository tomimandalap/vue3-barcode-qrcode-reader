{
  "name": "vue3-barcode-qrcode-reader",
  "version": "1.0.0",
  "description": "Vue 3 barcodes and QR codes scanner",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-barcode-qrcode-reader.umd.cjs",
  "module": "./dist/vue3-barcode-qrcode-reader.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-barcode-qrcode-reader.js",
      "require": "./dist/vue3-barcode-qrcode-reader.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "keywords": [
    "vue",
    "barcode",
    "qrcode",
    "zxing",
    "reader"
  ],
  "author": "tomimandalaputra <tommymandalaputra@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomimandalap/vue3-barcode-qrcode-reader/issues"
  },
  "homepage": "https://github.com/tomimandalap/vue3-barcode-qrcode-reader#readme",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^10.6.1",
    "@zxing/library": "^0.20.0",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vue-tsc": "^1.8.19"
  }
}
